                     <font>
                        <Font size="15.0"/>
                     </font>
                     <VBox.margin>
                        <Insets top="30.0"/>
                     </VBox.margin>
                  </Label>
                  <JFXButton fx:id="loginButton" prefHeight="45.0" prefWidth="90.0"
                             style="-fx-background-color: #a6a548; -fx-background-radius: 10;" text="Login"
                             textFill="WHITE">
                     <VBox.margin>
                        <Insets top="70.0"/>
                     </VBox.margin>
                     <font>
                        <Font name="System Bold" size="15.0"/>
                     </font>
                  </JFXButton>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</fx:root>
