                                unFocusColor="#4d4d4d00">
                     <VBox.margin>
                        <Insets left="50.0" right="50.0" top="105.0"/>
                     </VBox.margin>
                     <padding>
                        <Insets left="20.0" right="20.0"/>
                     </padding>
                     <font>
                        <Font size="14.0"/>
                     </font>
                  </JFXTextField>
                  <JFXPasswordField fx:id="passwordLogin" focusColor="#757575" prefHeight="45.0" promptText="Password"
                                    style="-fx-background-color: #ceb175; -fx-background-radius: 15;"
                                    unFocusColor="#4d4d4d00">
