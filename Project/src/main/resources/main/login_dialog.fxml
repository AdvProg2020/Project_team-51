                        <Insets left="50.0" right="50.0" top="20.0"/>
                     </VBox.margin>
                     <padding>
                        <Insets left="20.0" right="20.0"/>
                     </padding>
                     <font>
                        <Font size="14.0"/>
                     </font>
                  </JFXPasswordField>
                  <Label fx:id="loginError" alignment="CENTER" prefHeight="25.0" prefWidth="357.0" text="Login Error"
