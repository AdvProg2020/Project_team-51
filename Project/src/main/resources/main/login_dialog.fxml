                                      style="-fx-background-color: #ceb175; -fx-background-radius: 15;"
                                      unFocusColor="#4d4d4d00">
                           <padding>
                              <Insets left="20.0" right="20.0"/>
                           </padding>
                           <font>
                              <Font size="14.0"/>
