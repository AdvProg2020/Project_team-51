                                      style="-fx-background-color: #ceb175; -fx-background-radius: 15;"
                                      unFocusColor="#4d4d4d00" visible="false">
                           <padding>
                              <Insets left="20.0" right="20.0"/>
                           </padding>
                           <font>
                              <Font size="14.0"/>
                           </font>
                        </JFXTextField>
                     </children>
                     <VBox.margin>
                        <Insets left="40.0" right="40.0" top="20.0"/>
                     </VBox.margin>
                  </HBox>
                  <JFXButton fx:id="registerButton" prefHeight="45.0" prefWidth="90.0"
                             style="-fx-background-color: #a6a548; -fx-background-radius: 10;" text="Register"
                             textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="15.0"/>
                     </font>
                     <VBox.margin>
                        <Insets top="20.0"/>
                     </VBox.margin>
                  </JFXButton>
                  <Label fx:id="registerError" alignment="CENTER" prefHeight="25.0" prefWidth="357.0"
