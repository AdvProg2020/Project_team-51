<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.*?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" prefHeight="600.0" prefWidth="1200.0"
         style="-fx-background-color: #f3c669;" type="AnchorPane" xmlns="http://javafx.com/javafx/11.0.1">
   <children>
      <HBox prefHeight="550.0" prefWidth="1200.0">
         <children>
            <VBox alignment="TOP_CENTER" prefHeight="550.0" prefWidth="825.0">
               <children>
                  <Label prefHeight="100.0" text="Register" textFill="#9d9996">
                     <font>
                        <Font name="System Bold" size="41.0"/>
                     </font>
                     <VBox.margin>
                        <Insets/>
                     </VBox.margin>
                  </Label>
                  <HBox alignment="CENTER" prefWidth="200.0" spacing="50.0">
                     <VBox.margin>
                        <Insets left="40.0" right="40.0" top="20.0"/>
                     </VBox.margin>
                     <children>
                        <ComboBox fx:id="accountType" prefHeight="45.0" prefWidth="257.0" promptText="Account Type">
                           <padding>
                              <Insets left="10.0"/>
                           </padding>
                           <HBox.margin>
                              <Insets/>
                           </HBox.margin>
                        </ComboBox>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" prefWidth="200.0" spacing="50.0">
                     <children>
                        <JFXTextField fx:id="firstName" focusColor="#757575" prefHeight="45.0" prefWidth="220.0"
                                      promptText="First Name"
                                      style="-fx-background-color: #ceb175; -fx-background-radius: 15;"
                                      unFocusColor="#4d4d4d00">
                           <padding>
                              <Insets left="20.0" right="20.0"/>
                           </padding>
                           <font>
                              <Font size="14.0"/>
                           </font>
                        </JFXTextField>
                        <JFXTextField fx:id="lastName" focusColor="#757575" prefHeight="45.0" prefWidth="220.0"
                                      promptText="Last Name"
                                      style="-fx-background-color: #ceb175; -fx-background-radius: 15;"
                                      unFocusColor="#4d4d4d00">
                           <padding>
                              <Insets left="20.0" right="20.0"/>
                           </padding>
                           <font>
                              <Font size="14.0"/>
                           </font>
                        </JFXTextField>
                     </children>
                     <VBox.margin>
                        <Insets left="40.0" right="40.0" top="20.0"/>
                     </VBox.margin>
                  </HBox>
                  <HBox alignment="CENTER" prefWidth="200.0" spacing="50.0">
                     <children>
                        <JFXTextField fx:id="usernameRegister" focusColor="#757575" prefHeight="45.0" prefWidth="220.0"
                                      promptText="Username"
                                      style="-fx-background-color: #ceb175; -fx-background-radius: 15;"
                                      unFocusColor="#4d4d4d00">
                           <padding>
                              <Insets left="20.0" right="20.0"/>
                           </padding>
                           <font>
                              <Font size="14.0"/>
                           </font>
                        </JFXTextField>
                        <JFXTextField fx:id="passwordRegister" focusColor="#757575" prefHeight="45.0" prefWidth="220.0"
                                      promptText="Password"
                                      style="-fx-background-color: #ceb175; -fx-background-radius: 15;"
                                      unFocusColor="#4d4d4d00">
                           <padding>
                              <Insets left="20.0" right="20.0"/>
                           </padding>
                           <font>
                              <Font size="14.0"/>
                           </font>
                        </JFXTextField>
                     </children>
                     <VBox.margin>
                        <Insets left="40.0" right="40.0" top="20.0"/>
                     </VBox.margin>
                  </HBox>
                  <HBox alignment="CENTER" prefWidth="200.0" spacing="50.0">
                     <children>
                        <JFXTextField fx:id="email" focusColor="#757575" prefHeight="45.0" prefWidth="220.0"
