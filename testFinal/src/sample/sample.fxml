<GridPane xmlns:fx="http://javafx.com/fxml/1" alignment="center" hgap="10" styleClass="background"
          stylesheets="@style.css" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="sample.Controller">
    <children>
        <AnchorPane prefHeight="200.0" prefWidth="200.0" styleClass="background" stylesheets="@style.css">
            <children>
                <Button layoutX="65.0" layoutY="81.0" mnemonicParsing="false" stylesheets="@style.css" text="Button"/>
            </children>
        </AnchorPane>
    </children>
    <rowConstraints>
        <RowConstraints/>
    </rowConstraints>
    <columnConstraints>
        <ColumnConstraints/>
    </columnConstraints>
</GridPane>
